{
  "name": "ngx-extended-pdf-viewer",
  "description": "Embedding PDF files in your Angular application. Highly configurable viewer including the toolbar, sidebar, and all the features you're used to.",
  "version": "25.6.0-alpha.2",
  "license": "Apache-2.0 with Commons-Clause",
  "repository": {
    "url": "https://github.com/stephanrauh/ngx-extended-pdf-viewer"
  },
  "schematics": "./schematics/collection.json",
  "ng-add": {
    "save": "dependencies"
  },
  "author": {
    "name": "Stephan Rauh",
    "url": "https://www.beyondjava.net"
  },
  "homepage": "https://pdfviewer.net",
  "bugs": {
    "url": "https://github.com/stephanrauh/ngx-extended-pdf-viewer/issues"
  },
  "keywords": [
    "PDF",
    "pdfjs",
    "PDF viewer",
    "Angular",
    "ng2",
    "Angular 17 - 20",
    "customizable PDF viewer"
  ],
  "peerDependencies": {
    "@angular/common": ">=17.0.0 <21.0.0",
    "@angular/core": ">=17.0.0 <21.0.0"
  },
  "module": "fesm2022/ngx-extended-pdf-viewer.mjs",
  "typings": "index.d.ts",
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./index.d.ts",
      "esm2022": "./esm2022/ngx-extended-pdf-viewer.mjs",
      "esm": "./esm2022/ngx-extended-pdf-viewer.mjs",
      "default": "./fesm2022/ngx-extended-pdf-viewer.mjs"
    }
  },
  "sideEffects": false,
  "dependencies": {
    "tslib": "^2.3.0"
  }
}